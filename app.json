{
  "expo": {
    "name": "Sentreso Finance",
    "slug": "sentreso-finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sentreso",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSMicrophoneUsageDescription": "Cette application utilise le microphone pour enregistrer vos transactions vocales.",
        "NSSpeechRecognitionUsageDescription": "Cette application utilise la reconnaissance vocale pour convertir votre voix en texte lors de l’ajout de transactions.",
        "NSPhotoLibraryUsageDescription": "Cette application peut accéder à votre bibliothèque de photos si vous souhaitez ajouter des reçus ou des justificatifs à vos transactions.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.zizsalam.sentresofinance",
      "buildNumber": "25"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/images/icon.png",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.zizsalam.sentresofinance"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98d358f0-9c1a-4f1d-8df4-68c3da5e331d"
      }
    }
  }
}
